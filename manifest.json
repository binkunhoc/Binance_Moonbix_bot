{
    "manifest_version": 3,
    "name": "Auto Click Tasks Tab",
    "version": "1.0",
    "description": "Automatically click the task tab on Binance and complete tasks.",
    "permissions": [
      "activeTab",
      "scripting",
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://www.binance.com/*"],
        "js": ["contentScript.js"]
      }
    ]
  }
